      "metadata": {},
      "source": [
        "X = np.linspace(start=0, stop=13, num=1_000).reshape(-1, 1)\n",
        "X_train = np.array([1., 2., 3., 4., 12.]).reshape(-1,1)\n",
        "y_train = np.array([0., -1.75, -2, -0.5, 5.])"
